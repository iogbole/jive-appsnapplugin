                         description="admin.decorator.menu.system.appsnap.main.desc">
                    <item id="viewappsnap-Page" name="admin.decorator.menu.system.appsnap.view"
                          url="appsnap-view-previous.jspa"
                          description="admin.decorator.menu.system.appsnap.view.desc"/>
                    <item id="grabjvmappsnap-Page" name="admin.decorator.menu.system.appsnap.sub.menu"
