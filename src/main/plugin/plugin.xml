<plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://www.jivesoftware.com/schemas/clearspace/2_5/plugin.xsd">
    <name>appsnapplugin</name>
    <description>Grab Appnsap.
    </description>
    <author>Israel Ogbole</author>
    <version>1.0.0</version>
    <minServerVersion>5.0.3.0</minServerVersion>
    <maxServerVersion>6.0.4.0</maxServerVersion>

    <databaseKey>database-query</databaseKey>
    <databaseVersion>1050302</databaseVersion>

    <!-- you will want to change your class name here! -->
